function test_cunit() {
    const a = [0x12345607, 0x89ABCDEF];
    const u = EPDCUnitMap(EUDVariable(EPD(a) - 19));
    var ret = 0;
    if (u.order == 0x56) ret += 1;
    if (u.owner == P8) ret += 2;
    if (u.orderState == 0x34) ret += 4;
    if (u.orderSignal == 0x12) ret += 8;
    if (u.orderUnitType == 0xCDEF) ret += 16;
    wwrite_epd(EPD(a) + 1, 0, $U("Artanis"));
    if (u.orderUnitType == py_str("Artanis")) ret += 32;
    return ret;
}